<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="create-fks" author="Norbert Bodnar">
        <addForeignKeyConstraint
                baseTableName="baseline_test_case"
                baseColumnNames="system_under_test_id"
                constraintName="fk_baseline_test_case_system_under_test"
                referencedTableName="system_under_test"
                referencedColumnNames="id"/>
        <dropForeignKeyConstraint baseTableName="system_under_test" constraintName="fk_system_under_test_benchmark"/>
    </changeSet>
</databaseChangeLog>
